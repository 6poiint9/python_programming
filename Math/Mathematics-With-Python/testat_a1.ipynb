   "source": [
    "# Tupel mit den Werten\n",
    "fruechte = (\"Apfel\", \"Birne\", \"Banane\")\n",
    "\n",
    "# Index-Variable f√ºr while-Schleife\n",
    "i = 0\n",
    "\n",
    "# while-Schleife zum Ausgeben\n",
    "while i < len(fruechte):\n",
    "    print(f\"{i + 1}. Wert: {fruechte[i]}\")\n",
    "    i += 1\n"
   ],
